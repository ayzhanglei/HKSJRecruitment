<div id="winEdit" class="easyui-window" title="title" data-options="closed:true,closable:true,collapsible:false,minimizable:false,maximizable:false,modal:true,iconCls:'icon-add'" style="width: 420px; height:470px; padding: 5px; background: #fafafa;">
    <form id="frmEdit" method="post">
        <input type="hidden" value="0" id="id" name="id" />
        <table style="margin-left:auto;margin-right:auto;width:90%;">
            <tr>
                <td><label class="easyui-label">职务：</label></td>
                <td>
                    <input id="positionname" name="positionname" type="text" class="easyui-control easyui-validatebox" data-options="required:true" />
                </td>
            </tr>
            <tr>
                <td><label class="easyui-label">级别：</label></td>
                <td><input id="positionlevel" name="positionlevel" style="height:34px;" class="easyui-control easyui-numberspinner" required="required" data-options="min:1,max:100,editable:true" /></td>
            </tr>
   
        </table>

        <div class="form-group">
            <div class="easyui-l-btn">
                <a id="btnWinEditOk" class="easyui-linkbutton easyui-btn" href="javascript:void(0)">确定</a>
                <a id="btnWinEditCancel" class="easyui-linkbutton easyui-btn" href="javascript:void(0)">关闭</a>
            </div>
        </div>
    </form>
</div>